{
  "name": "modulik",
  "private": "true",
  "workspaces": [
    "modulik"
  ],
  "scripts": {
    "build": "rimraf modulik/lib && rollup -c",
    "build:watch": "rimraf modulik/lib && rollup -c --watch",
    "test": "mocha test/main/*.spec.js && yarn --cwd test/transpilation start",
    "lint": "eslint modulik/src",
    "example": "nodemon -q -w src -x \"yarn --cwd example start\"",
    "postinstall": "yarn --cwd test/transpilation install"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/chokidar": "^2.1.3",
    "@types/node": "^17.0.21",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-eslint": "^10.0.2",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "fast-glob": "^3.2.11",
    "husky": "^3.0.0",
    "lint-staged": "^9.0.2",
    "mocha": "^6.2.2",
    "nodemon": "2.0.12",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "rollup": "^2.70.1",
    "rollup-plugin-copy": "^3.4.0",
    "typescript": "^4.6.3"
  }
}
