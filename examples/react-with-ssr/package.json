{
  "name": "modulik-example",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "run-p build:*",
    "build:client": "NODE_ENV=production webpack --config webpack.config.js",
    "build:server": "babel -d dist \"src\"",
    "start": "nodemon -x babel-node -w src/server/index.js src/server -- 3000",
    "start:prod": "NODE_ENV=production node dist/server 3000"
  },
  "dependencies": {
    "express": "^4.17.1",
    "modulik": "../../modulik",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/node": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/register": "^7.16.7",
    "babel-loader": "^8.0.6",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-middleware": "^3.7.0"
  }
}
